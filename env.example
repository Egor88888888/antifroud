# Общие настройки
ENV=production
APP_VERSION=1.0.0

# Настройки базы данных
POSTGRES_USER=crafd
POSTGRES_PASSWORD=crafd_secret
POSTGRES_DB=crafd_db
DATABASE_URL=postgresql://crafd:crafd_secret@postgres:5432/crafd_db

# Настройки Redis
REDIS_URL=redis://redis:6379

# Настройки Neo4j
NEO4J_PASSWORD=neo4j_password
NEO4J_URI=bolt://neo4j:7687

# Настройки безопасности
JWT_SECRET=your_jwt_secret_key_should_be_long_and_secure
SESSION_SECRET=your_session_secret_key_should_be_long_and_secure

# Настройки API
API_URL=https://api.warevision-antifraud.example.com

# Настройки для разработки
DEBUG=false
LOG_LEVEL=info 